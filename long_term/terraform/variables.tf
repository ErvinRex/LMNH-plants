# Variable secrets on local terraform.tfvars file
variable "AWS_KEY" {
    type = string
}

variable "AWS_SKEY" {
    type = string
}

variable "DB_USER" {
    type = string
}

variable "DB_PASSWORD" {
    type = string
}

variable "DB_NAME" {
    type = string
}

variable "DB_HOST" {
    type = string
}

variable "DB_PORT" {
    type = string
}

variable "DB_SCHEMA" {
    type = string
}